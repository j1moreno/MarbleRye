<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.j1.marblerye">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/marblerye_launcher9"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/marblerye_launcher9_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReadDBActivity" />
        <activity android:name=".NewExpenseInput" />
        <activity android:name=".EditExpenseActivity" />
        <activity android:name=".ViewHistoryActivity" />
        <activity android:name=".DetailedExpenseHistoryActivity" />
        <activity android:name=".AboutActivity"/>
    </application>

</manifest>